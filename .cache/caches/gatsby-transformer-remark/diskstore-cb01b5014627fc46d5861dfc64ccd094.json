nt","tagName":"strong","properties":{},"children":[{"type":"text","value":"Ease of navigation","position":{"start":{"line":37,"column":5,"offset":2095},"end":{"line":37,"column":23,"offset":2113}}}],"position":{"start":{"line":37,"column":3,"offset":2093},"end":{"line":37,"column":25,"offset":2115}}}],"position":{"start":{"line":37,"column":3,"offset":2093},"end":{"line":37,"column":31,"offset":2121}}},{"type":"text","value":"\n"},{"type":"element","tagName":"td","properties":{"align":null},"children":[{"type":"text","value":"Simple navigation consisting of taps and scrolls","position":{"start":{"line":37,"column":34,"offset":2124},"end":{"line":37,"column":82,"offset":2172}}}],"position":{"start":{"line":37,"column":34,"offset":2124},"end":{"line":37,"column":118,"offset":2208}}},{"type":"text","value":"\n"}],"position":{"start":{"line":37,"column":1,"offset":2091},"end":{"line":37,"column":120,"offset":2210}}},{"type":"text","value":"\n"}],"position":{"start":{"line":34,"column":1,"offset":1680},"end":{"line":37,"column":120,"offset":2210}}},{"type":"text","value":"\n"}],"position":{"start":{"line":32,"column":1,"offset":1440},"end":{"line":37,"column":120,"offset":2210}}},{"type":"text","value":"\n"},{"type":"raw","value":"<div class=\"csblock\" id=\"design\" style=\"margin-bottom: 10px\"> \n<span class=\"csblockheading\">\n    Design\n</span>\n</div>","position":{"start":{"line":39,"column":1,"offset":2212},"end":{"line":43,"column":7,"offset":2330}}},{"type":"text","value":"\n"},{"type":"raw","value":"<div class=\"csblock\" style=\"margin-top: 10px; margin-bottom: 10px;\"> \n    <b>Garden:</b> I was responsible for creating graphical contents, as well as exploring the interactions for the garden page. In the garden, kid users can water their plants and access a market to purchase plants with in-app currency. They can also access their friend’s garden and water their friend’s plants. \n</div>","position":{"start":{"line":45,"column":1,"offset":2332},"end":{"line